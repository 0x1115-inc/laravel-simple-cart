{
    "name": "mcxv/laravel-simple-cart",
    "description": "A simple shopping cart package for Laravel",
    "type": "library",
    "license": "Apache 2.0",
    "authors": [
        {
            "name": "Duog Lee",
            "email": "duoglee@0x1115.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "MCXV\\SimpleCart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MCXV\\SimpleCart\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MCXV\\SimpleCart\\SimpleCartServiceProvider"
            ],
            "aliases": {
                "Cart": "MCXV\\SimpleCart\\Facades\\Cart"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}